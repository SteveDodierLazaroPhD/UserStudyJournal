<?xml version="1.0"?>
<gconfschemafile>
  <schemalist>
    <schema>
      <key>/schemas/apps/ucl-study-journal/window_width</key>
      <applyto>/apps/ucl-study-journal/window_width</applyto>
      <owner>ucl-study-journal</owner>
      <type>int</type>
      <locale name="C">
        <short>Window width</short>
        <long>Customized width for the main window, in pixels (requires restart). Can't be bigger than the desktop's size.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/ucl-study-journal/window_height</key>
      <applyto>/apps/ucl-study-journal/window_height</applyto>
      <owner>ucl-study-journal</owner>
      <type>int</type>
      <locale name="C">
        <short>Window height</short>
        <long>Customized height for the main window, in pixels (requires restart). Can't be bigger than the desktop's size.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/ucl-study-journal/amount_days</key>
      <applyto>/apps/ucl-study-journal/amount_days</applyto>
      <owner>ucl-study-journal</owner>
      <type>int</type>
      <locale name="C">
        <short>Amount of visible days</short>
        <long>The amount of days to show at once.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/ucl-study-journal/accessibility</key>
      <applyto>/apps/ucl-study-journal/accessibility</applyto>
      <owner>ucl-study-journal</owner>
      <type>bool</type>
      <default>False</default>
      <locale name="C">
        <short>Accessibility</short>
        <long>Whether special accessibility options should be enabled (requires restart).</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/ucl-study-journal/small_thumbnails</key>
      <applyto>/apps/ucl-study-journal/small_thumbnails</applyto>
      <owner>ucl-study-journal</owner>
      <type>bool</type>
      <default>False</default>
      <locale name="C">
        <short>Allow thumbnails as icons</short>
        <long>Whether to allow showing little thumbnails next to each activity (requires restart). If False, only generic mime-type icons will be used.</long>
      </locale>
    </schema>
  </schemalist>
</gconfschemafile>
